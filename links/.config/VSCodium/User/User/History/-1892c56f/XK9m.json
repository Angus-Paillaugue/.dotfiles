{
  "manifest_version": 3,
  "name": "Redirector",
  "version": "1.0",
  "description": "Redirects you from google website to open-source alternatives",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "redirector@paillaugue.fr"
    }
  }
}

{
  "manifest_version": 2,
  "name": "Redirector",
  "version": "1.0",
  "description": "Redirects you from google website to open-source alternatives",
  "content_scripts": [
    {
      "matches": [
        "*"
      ],
      "js": [
        "borderify.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "redirector@paillaugue.fr"
    }
  }

}
